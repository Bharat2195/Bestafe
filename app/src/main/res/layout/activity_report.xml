                        android:layout_width="120dp"
                        android:layout_height="50dp"
                        android:background="#ccc"
                        android:gravity="center"
                        android:padding="7dp"
                        android:text="1"
                        android:textColor="#000"
                        android:textSize="13sp" />

                    <!--<TextView-->
                    <!--android:id="@+id/txtSrNo"-->
