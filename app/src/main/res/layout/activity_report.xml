                        android:textColor="#000"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/tv_propertylistitem_tenentname2"
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:background="#f5f5f5"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="BESTAFE ONLINE"
                        android:textColor="#000"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/tv_propertylistitem_contactno2"
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:background="#f5f5f5"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="9558915152"
                        android:textColor="#000"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/tv_propertylistitem_amount2"
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:background="#f5f5f5"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="21-July-2016"
                        android:textColor="#000"
                        android:textSize="13sp" />

                    <TextView
                        android:id="@+id/tv_Activation_Date2"
                        android:layout_width="100dp"
                        android:layout_height="50dp"
                        android:background="#f5f5f5"
                        android:gravity="center"
                        android:padding="5dp"
                        android:text="21-July-2016"
                        android:textColor="#000"
