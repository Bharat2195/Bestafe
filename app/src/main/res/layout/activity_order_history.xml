<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_order_history"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.toranado.bestafe.OrderHistoryActivity">

    <include
        android:id="@+id/toolbar_order_history"
        android:layout_width="match_parent"
        layout="@layout/toolbar_login_main"
        android:layout_height="?attr/actionBarSize">

    </include>


    <ListView
        android:id="@+id/listview_order_history"
        android:layout_below="@+id/toolbar_order_history"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </ListView>


</RelativeLayout>
